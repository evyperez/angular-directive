{
    "name": "angular-directive",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --configuration production",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "lint": "npm-run-all --parallel lint:*",
        "lint:sass": "npx stylelint src/stylesheets/**/*.scss --syntax=scss --formatter --output-file ./testResults/stylelint.xml",
        "sass:fix": "npx stylelint src/stylesheets/**/*.scss --syntax=scss --fix",
        "lint:es": "ng lint",
        "lint-staged": "ng lint $(git diff --name-only --cached | grep -E '\\.ts|\\.html' | sed 's/^/ --lint-file-patterns /')",
        "lint:fix": "ng lint --fix",
        "prettier:check": "./node_modules/.bin/prettier --check \"src/app/**/*.ts\"",
        "prettier:write": "prettier --write ."
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.1.0",
        "@angular/common": "~13.1.0",
        "@angular/compiler": "~13.1.0",
        "@angular/core": "~13.1.0",
        "@angular/forms": "~13.1.0",
        "@angular/platform-browser": "~13.1.0",
        "@angular/platform-browser-dynamic": "~13.1.0",
        "@angular/router": "~13.1.0",
        "rxjs": "~7.4.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.1.4",
        "@angular/cli": "~13.1.4",
        "@angular/compiler-cli": "~13.1.0",
        "@types/jasmine": "~3.10.0",
        "@types/node": "^12.11.1",
        "jasmine-core": "~3.10.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "typescript": "~4.5.2",
        "@typescript-eslint/eslint-plugin": "5.11.0",
        "@typescript-eslint/parser": "5.11.0",
        "eslint": "^8.2.0",
        "eslint-plugin-prettier": "^4.0.0",
        "modular-scale": "^5.1.2",
        "modularscale-sass": "^3.0.10",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.6.1",
        "sassdoc": "^2.7.3",
        "sassdoc-theme-herman": "^4.0.2",
        "stylelint": "^13.13.1",
        "stylelint-config-recess-order": "^2.4.0",
        "stylelint-config-recommended": "^5.0.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-sass-guidelines": "^8.0.0",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-scss": "^3.19.0",
        "stylelint-selector-bem-pattern": "^2.1.0",
        "@angular-eslint/builder": "13.1.0",
        "@angular-eslint/eslint-plugin": "13.1.0",
        "@angular-eslint/eslint-plugin-template": "13.1.0",
        "@angular-eslint/schematics": "13.1.0",
        "@angular-eslint/template-parser": "13.1.0"
    }
}
